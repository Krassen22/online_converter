<%= form_for @request do |f| %>
		<div>
			<%= f.label :source_url %>
			<%= f.text_field :source_url, value: "https://www.sit.auckland.ac.nz/wiki/images/9/9f/Sample.flv" %>
		</div>
		<div>
			<%= f.label :media_type %>
			<%= f.text_field :media_type, value: "video" %>
		</div>
		<div>
			<%= f.label :convert_to %>
			<%= f.text_field :convert_to, value: "mp4" %>
		</div>
		<%= f.submit %>
<% end %>
